double alan (double uzunKenar,double kisaKenar)
{
    return uzunKenar*kisaKenar;
}

int carp(int a,int b)
{
   

   
   int multiplyByTwo(int x)
    {
        return x*2;
    }
    while(a+1==b)
    {
 return a*2^(b-1);
    }


}
List <String> silme(List<String> liste,String eleman)
{
    liste.remove(eleman);
    return liste;
}
class sekil
{
